<section class="status">
         <div class="container mx-auto">
                  <div class="status-box w-full lg:w-2/3 mx-auto">
                           <div class="flex items-center justify-between mb-12">
                                    <h1 class="text-xl font-bold">Track delivery status</h1>
                                    <h6 class="bg-white py-1 rounded-full px-4 text-green-600 text-xs"><%=
                                             order._id %></h6>
                                    <input id="hiddenInput" type="hidden" value="<%= JSON.stringify(order) %>">
                           </div>
                           <ul>
                                    <li class="status_line text-sm md:text-xl pb-16" data-status="order_placed"><span>Order Placed<i class="bi bi-check"></i></span>
                                    </li>
                                    <li class="status_line text-sm md:text-xl pb-16" data-status="confirmed"><span>Order confirmation<i class="bi bi-check"></i></span>
                                    </li>
                                    <li class="status_line text-sm md:text-xl pb-16" data-status="prepared"><span>Preparation<i class="bi bi-check"></i></span>
                                    </li>
                                    <li class="status_line text-sm md:text-xl pb-16" data-status="delivered"><span>Out for delivery<i class="bi bi-check"></i></span>
                                    </li>
                                    <li class="status_line text-sm md:text-xl" data-status="completed"><span>Complete<i class="bi bi-check"></i></span>
                                    </li>
                                    <button id="logbt" class="py-6 px-2 btn-primary rounded-full">Console.log</button>
                           </ul>
                  </div>
         </div>
</section>